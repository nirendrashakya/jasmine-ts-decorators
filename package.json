{
  "name": "jasmine-ts-decorators",
  "version": "2.0.1",
  "description": "Jasmine Typescript Decorators",
  "main": "dist/index.js",
  "scripts": {
    "test": "jasmine-ts --config=spec/config/jasmine.json",
    "test:watch": "nodemon --ext ts --exec npm test",
    "test:cov": "nyc npm run test",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc"
  },
  "keywords": [
    "jasmine",
    "typescript",
    "decorators",
    "data driven testing",
    "jasmine-ts"
  ],
  "author": "Nirendra Shakya <nirendrashakya@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nirendrashakya/jasmine-ts-decorators.git",
    "web": "https://github.com/nirendrashakya/jasmine-ts-decorators"
  },
  "license": "ISC",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "spec/lib/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "check-coverage": true,
    "per-file": true,
    "functions": 100
  },
  "dependencies": {
    "jasmine": "3.4.0",
    "jasmine-ts": "0.3.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/jasmine": "^3.3.13",
    "jasmine-spec-reporter": "4.2.1",
    "nodemon": "1.19.1",
    "nyc": "^14.1.1",
    "ts-node": "3.2.0",
    "typescript": "3.5.1"
  }
}
